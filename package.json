{
    "name": "obsidian-spaced-repetition",
    "version": "1.12.6",
    "description": "Fight the forgetting curve by reviewing flashcards & entire notes.",
    "main": "main.js",
    "scripts": {
        "build": "node esbuild.config.mjs production",
        "dev": "node esbuild.config.mjs",
        "format": "pnpm prettier --write . && pnpm eslint src/ tests/ --fix",
        "lint": "pnpm prettier --check . && pnpm eslint src/ tests/",
        "test": "pnpm jest",
        "changelog": "pnpm auto-changelog --template=compact --package && pnpm prettier --write docs/docs/changelog.md",
        "e2e": "make setup_e2e && wdio run ./wdio.conf.js"
    },
    "keywords": [
        "obsidian",
        "spaced repetition",
        "flashcard",
        "flashcards"
    ],
    "author": "Stephen Mwangi",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.11.0",
        "@types/eslint__js": "^8.42.3",
        "@types/jest": "^29.5.13",
        "@types/node": "^22.5.5",
        "@types/vhtml": "^2.2.9",
        "@typescript-eslint/eslint-plugin": "^8.6.0",
        "@typescript-eslint/parser": "^8.6.0",
        "@wdio/cli": "^9.0.9",
        "@wdio/local-runner": "^9.0.9",
        "@wdio/mocha-framework": "^9.0.8",
        "@wdio/spec-reporter": "^9.0.8",
        "auto-changelog": "^2.5.0",
        "builtin-modules": "^4.0.0",
        "chai": "^5.1.1",
        "esbuild": "^0.23.1",
        "eslint": "^9.11.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unicorn": "^55.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-expect-message": "^1.1.3",
        "moment": "^2.30.1",
        "obsidian": "^1.7.2",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.5",
        "tslib": "^2.7.0",
        "typescript": "~5.5.4",
        "typescript-eslint": "^8.6.0",
        "vhtml": "^2.2.0",
        "wdio-chromedriver-service": "^8.1.1"
    },
    "dependencies": {
        "chart.js": "^4.4.4",
        "pagerank.js": "^1.0.2",
        "peggy": "^4.0.3"
    },
    "packageManager": "^pnpm@9.10.0"
}
