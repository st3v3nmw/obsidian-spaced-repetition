{
    "name": "obsidian-spaced-repetition",
    "version": "1.12.5",
    "description": "Fight the forgetting curve by reviewing flashcards & entire notes.",
    "main": "main.js",
    "scripts": {
        "build": "node esbuild.config.mjs production",
        "dev": "node esbuild.config.mjs",
        "format": "npx prettier --write .",
        "lint": "npx prettier --check . && npx eslint src/",
        "test": "jest",
        "changelog": "auto-changelog --template=compact --package && npx prettier --write docs/changelog.md",
        "e2e": "make setup_e2e && wdio run ./wdio.conf.js"
    },
    "keywords": [
        "obsidian",
        "spaced-repetition",
        "flashcards"
    ],
    "author": "Stephen Mwangi",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.16.1",
        "@types/vhtml": "^2.2.9",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@wdio/cli": "^8.40.3",
        "@wdio/local-runner": "^8.40.3",
        "@wdio/mocha-framework": "^8.40.3",
        "@wdio/spec-reporter": "^8.40.3",
        "auto-changelog": "^2.4.0",
        "builtin-modules": "^3.3.0",
        "chai": "^4.5.0",
        "esbuild": "~0.19.12",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-expect-message": "^1.1.3",
        "moment": "^2.30.1",
        "obsidian": "^1.6.6",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.5",
        "tslib": "2.6.1",
        "typescript": "5.1.6",
        "vhtml": "^2.2.0",
        "wdio-chromedriver-service": "^8.1.1"
    },
    "dependencies": {
        "chart.js": "^4.4.4",
        "pagerank.js": "^1.0.2",
        "peggy": "^4.0.3"
    },
    "packageManager": "^pnpm@8.15.4"
}
